{
    "version": 2,
    "builds": [
      {
        "src": "food-dell-backend/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/build/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/food-dell-backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/client/build/$1"
      }
    ]
  }
  